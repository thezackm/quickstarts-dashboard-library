                        "height": 3,
                        "width": 4
                    },
                    "title": "Requests per second (ALB)",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT rate(sum(`aws.applicationelb.RequestCount.byAlb`), 1 minute) as 'requests/second' FROM Metric WHERE collector.name='cloudwatch-metric-streams' since 60 minutes ago"
                            }
