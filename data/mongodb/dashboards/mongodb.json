                        "width": 8
                    },
                    "title": "Database Indexes",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT latest(`stats.indexes`) AS `Indexes`, latest(`stats.indexInBytes`) AS `Total Index Size`  FROM MongoDatabaseSample FACET entityName LIMIT 1000"
                            }
                        ]
                    }
                },
                {
                    "visualization": {
