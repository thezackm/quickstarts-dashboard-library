                {
                    "visualization": {
                        "id": "viz.line"
                    },
                    "layout": {
                        "column": 1,
                        "row": 13,
                        "height": 3,
                        "width": 4
                    },
                    "title": "Time Running Commands",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "accountId": 0,
                                "query": "SELECT average(`usage.totalInMillisecondsPerSecond`) FROM MongodTopSample TIMESERIES 1 minutes FACET entityName"
                            }
                        ]
                    }
                },
                {
                    "visualization": {
                        "id": "viz.table"
